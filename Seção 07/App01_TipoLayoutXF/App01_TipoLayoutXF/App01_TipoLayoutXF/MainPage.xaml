<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:App01_TipoLayoutXF"
             x:Class="App01_TipoLayoutXF.MainPage">

    <StackLayout>
        <Button Text="Absolute" Clicked="GoPageAbsolute"/>
        <Button Text="Grid" Clicked="GoPageGrid"/>
        <Button Text="Relative" Clicked="GoPageRelative"/>
        <Button Text="Scroll" Clicked="GoPageScroll"/>
        <Button Text="Stack" Clicked="GoPageStack"/>
    </StackLayout>

</ContentPage>
